{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint --write . --config-path=biome.jsonc",
    "lint:check": "biome lint . --config-path=biome.jsonc",
    "format": "biome format --write . --config-path=biome.jsonc",
    "format:check": "biome format . --config-path=biome.jsonc",
    "check": "biome check --write --unsafe . --config-path=biome.jsonc",
    "check:check": "biome check . --config-path=biome.jsonc",
    "preview": "vite preview",
    "updatePackages:major": "npx taze major --includeLocked --write && npm install",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "date-fns": "^4.1.0",
    "maplibre-gl": "^5.16.0",
    "nuqs": "^2.8.6",
    "pmtiles": "^4.3.2",
    "zustand": "^5.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-map-gl": "^8.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "zod": "^4.3.5"
  }
}
